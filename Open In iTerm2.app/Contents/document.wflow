				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>运行AppleScript</string>
				<key>ActionNameComment</key>
				<string>获取当前打开的Finder目录名称</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>-- https://apple.stackexchange.com/questions/110778/open-new-tab-in-iterm-and-execute-command-there/360827-- https://talk.automators.fm/t/opening-a-new-terminal-tab-and-running-a-command-in-it/4839/4on is_running(appName)	tell application "System Events" to (name of processes) contains appNameend is_runningon iTermNewTabs(fileList, isFirstNewTab)	repeat with b from 1 to length of fileList		set currentItem to item b of fileList		if currentItem ≠ null then			if b ≠ 1 or isFirstNewTab = true then				tell application "System Events" to keystroke "t" using command down				delay 0.3			end if						tell application "iTerm"				tell current session of the last tab of current window					write text the POSIX path of currentItem				end tell			end tell		end if	end repeatend iTermNewTabson run {input, parameters}	-- convert input to list	set openList to {}	repeat with a from 1 to length of input		set currentItem to item a of input		set the end of openList to currentItem	end repeat		-- get current finder opened dir	tell application "Finder"		if exists Finder window 1 then			try				set currentDir to target of Finder window 1 as alias			on error				set currentDir to null			end try		else			set currentDir to null		end if	end tell		-- merge if no selected files	if length of input = 0 then		set the end of openList to currentDir	end if		-- filter to remove empty item	set finalList to {}	repeat with b from 1 to length of openList		set currentItem to item b of openList		if currentItem ≠ null then			set the end of finalList to currentItem		end if	end repeat		set iTermRunning to is_running("iTerm2")	if iTermRunning then		-- running		activate application "iTerm"		iTermNewTabs(finalList, true)	else		-- not running		tell application "iTerm"			launch session "Default Session"		end tell		delay 0.5		iTermNewTabs(finalList, false)	end if		return finalListend run</string>
